{
  "name": "spectacle-boilerplate",
  "version": "1.0.1",
  "description": "ReactJS Powered Presentation Framework",
  "main": "lib/index.js",
  "scripts": {
    "start": "NODE_ENV=development npm-run-all clean:dev copy:dev server",
    "deploy": "NODE_ENV=production npm-run-all clean:prod copy:prod build",
    "clean:dev": "rm -rf public/assets",
    "clean:prod": "rm -rf dist",
    "copy:dev": "cpx './vendor/**/*' public/assets --verbose",
    "copy:prod": "cpx './vendor/**/*' dist/assets --verbose",
    "build": "webpack -p --profile --colors",
    "server": "node server.js",
    "lint": "eslint --ext .js,.jsx ."
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "html-webpack-harddisk-plugin": "^0.1.0",
    "normalize.css": "3.0.3",
    "npm-watch": "^0.1.8",
    "react": "15.4.2",
    "react-dom": "15.4.2",
    "react-load-script": "^0.0.2",
    "spectacle": "^2.0.0",
    "webpack-merge": "^4.1.0"
  },
  "devDependencies": {
    "babel-cli": "^6.16.0",
    "babel-core": "^6.18.0",
    "babel-eslint": "^7.0.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-react-transform": "^2.0.0-beta1",
    "babel-plugin-transform-decorators-legacy": "^1.2.0",
    "babel-plugin-transform-react-constant-elements": "^6.4.0",
    "babel-plugin-transform-react-inline-elements": "^6.4.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.2.1",
    "babel-plugin-transform-runtime": "^6.4.3",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "babel-runtime": "^6.18.0",
    "cpx": "^1.5.0",
    "cross-env": "^1.0.7",
    "css-loader": "^0.23.0",
    "eslint": "^3.19.0",
    "eslint-config-formidable": "^2.0.1",
    "eslint-plugin-filenames": "^1.1.0",
    "eslint-plugin-import": "^2.0.1",
    "eslint-plugin-react": "^6.4.1",
    "express": "^4.13.3",
    "file-loader": "^0.9.0",
    "html-loader": "^0.4.0",
    "html-webpack-plugin": "^2.28.0",
    "is-buffer": "^1.1.1",
    "markdown-loader": "^0.1.7",
    "node-libs-browser": "^0.5.3",
    "npm-run-all": "^4.0.2",
    "raw-loader": "^0.5.1",
    "react-transform-catch-errors": "^1.0.0",
    "react-transform-hmr": "^1.0.4",
    "redbox-react": "^1.2.0",
    "rimraf": "^2.4.4",
    "style-loader": "^0.13.0",
    "surge": "latest",
    "url-loader": "^0.5.6",
    "webpack": "2.2.0",
    "webpack-dev-middleware": "^1.8.4",
    "webpack-hot-middleware": "^2.13.0"
  }
}
